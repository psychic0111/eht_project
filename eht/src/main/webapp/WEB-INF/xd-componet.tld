<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
                        "http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd">
<taglib>
 <tlib-version>1.0</tlib-version>
 <jsp-version>1.2</jsp-version>
 <short-name>xdc</short-name>
 <uri>http://www.xd-tech.com.cn/</uri>
 <description></description>
 <tag>
 	<name>pager</name>
 	<tag-class>com.eht.common.page.PageTag</tag-class>
 	<description>分页组件。</description>
 	<attribute>
 		<name>showTextPager</name>
 		<required>false</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>是否显示文本分页,默认true。</description>
 	</attribute>
 	<attribute>
 		<name>showNumerPager</name>
 		<required>false</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>是否显示数字分页,默认true。</description>
 	</attribute>
 	<attribute>
 		<name>offset</name>
 		<required>false</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>数字分页相对当前页的偏移量，默认值是2，如果当前页是3，那么页面将显示1 2 3 4 5。</description>
 	</attribute>
 	<attribute>
 		<name>htmlTag</name>
 		<required>false</required>
 		<rtexprvalue>false</rtexprvalue>
 		<description>分页使用的HTML标签，默认使用A标签。</description>
 	</attribute>
 	<attribute>
 		<name>pagerTheme</name>
 		<required>false</required>
 		<rtexprvalue>分页使用的样式表(类样式)。</rtexprvalue>
 	</attribute>
 	<attribute>
 		<name>curPagerTheme</name>
 		<required>false</required>
 		<rtexprvalue>当前使用的样式表(类样式)。</rtexprvalue>
 	</attribute>
 	<attribute>
 		<name>pagerTheme</name>
 		<required>false</required>
 		<rtexprvalue>分页使用的样式表(类样式)。</rtexprvalue>
 	</attribute>
 	<attribute>
 		<name>curPagerTheme</name>
 		<required>false</required>
 		<rtexprvalue>当前使用的样式表(类样式)。</rtexprvalue>
 	</attribute>
 	<attribute>
 		<name>pagerStyle</name>
 		<required>false</required>
 		<rtexprvalue>false</rtexprvalue>
 		<description>如果你不想使用一个类样式,可以用一个内嵌的样式来修饰分页标签。</description>
 	</attribute>
 	<attribute>
 		<name>curPagerTheme</name>
 		<required>false</required>
 		<rtexprvalue>false</rtexprvalue>
 		<description>如果你不想使用一个类样式,可以用一个内嵌的样式来修当前页。</description>
 	</attribute>
 	<attribute>
 		<name>pagerFunction</name>
 		<required>true</required>
 		<description>分页函数，用来控制分页行为，可接受一个页码参数。</description>
 	</attribute>
 </tag>
 <tag>
 	<name>hasPermission</name>
 	<tag-class>com.eht.common.util.PermissionTag</tag-class>
 	<description>系统标签组件</description>
 	<attribute>
 		<name>resource</name>
 		<required>true</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>鉴权资源名称，查看resource-actions目录下定义</description>
 	</attribute>
 	<attribute>
 		<name>action</name>
 		<required>true</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>鉴权资源ACTION，查看resource-actions目录下定义</description>
 	</attribute>
 	<attribute>
 		<name>primaryKey</name>
 		<required>false</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>数据主键</description>
 	</attribute>
 	<attribute>
 		<name>subjectId</name>
 		<required>true</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>专题ID</description>
 	</attribute>
 </tag>
 <tag>
 	<name>inBlackList</name>
 	<tag-class>com.eht.common.util.BlackListTag</tag-class>
 	<description>系统标签组件</description>
 	<attribute>
 		<name>resource</name>
 		<required>true</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>资源名称,directory or note</description>
 	</attribute>
 	<attribute>
 		<name>primaryKey</name>
 		<required>true</required>
 		<rtexprvalue>true</rtexprvalue>
 		<description>数据主键</description>
 	</attribute>
 </tag>
</taglib>
