package com.eht.common.constant;


public enum ResponseCode {
	
	NEXT_RECORD(200){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端上传下一条记录!";
		}
	},
	
	NEXT_SUBJECT(210){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端上传下一条记录!";
		}
	},
	
	NEXT_DIRECTORY(211){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端上传下一条记录!";
		}
	},
	
	NEXT_TAG(212){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端上传下一条记录!";
		}
	},
	
	NEXT_NOTE(213){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端上传下一条记录!";
		}
	},
	
	NEXT_ATTACHMENT(214){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端上传下一条记录!";
		}
	},
	
	FINISHED(300){
		@Override
		public String getMessage() {
			return "数据处理完成，同步结束!";
		}
	},
	
	PART_SYNCH_FINISHED(301){
		@Override
		public String getMessage() {
			return "此阶段数据同步已完成!";
		}
	},
	
	NEED_UPDATE(201){
		@Override
		public String getMessage() {
			return "数据处理完成，客户端需下载该数据更新!";
		}
	},
	
	NEED_DELETE(202){
		@Override
		public String getMessage() {
			return "数据处理完成，该数据已被删除!";
		}
	},
	
	NEED_SYNCH_ATTACHMENT(203){
		@Override
		public String getMessage() {
			return "数据处理完成，需同步本条目附件!";
		}
	},
	
	NEED_SYNCH_SUBJECTUSER(204){
		@Override
		public String getMessage() {
			return "多人专题更新完成，需同步专题成员!";
		}
	},
	
	NEED_SUBJECT_DATA(205){
		@Override
		public String getMessage() {
			return "多人专题添加完成，需同步本专题下所有数据!";
		}
	},
	
	NOT_COMPLETED(299){
		@Override
		public String getMessage() {
			return "接受和处理、但处理未完成!";
		}
	},
	SERVER_ERROR(500){
		@Override
		public String getMessage() {
			return "服务器处理发生异常!";
		}
	}, 
	AUTHORIZE_FAILED(403){
		@Override
		public String getMessage() {
			return "用户验证失败!";
		}
	}, 
	DATA_NOT_FOUND(404){
		@Override
		public String getMessage() {
			return "数据未找到!";
		}
	};
	
	private int code;
	
	private ResponseCode(int code) {
		this.code = code;
	}
	
	public int getCode() {
        return code;
    }
	
	public String getMessage(){
		return "同步操作完成！";
	}

}
