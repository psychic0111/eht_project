package com.eht.common.enumeration;

import com.eht.common.constant.SynchConstants;

public enum HeaderName {
	/**
	 * 客户端header中clientid的name值
	 */
	CLIENT_ID(SynchConstants.HEADER_CLIENT_ID),
	
	/**
	 * 返回客户端header中dataType的name值
	 */
	DATATYPE(SynchConstants.HEADER_DATATYPE),
	
	/**
	 * 返回客户端header中action的name值
	 */
	ACTION(SynchConstants.HEADER_ACTION),
	
	/**
	 * 返回客户端header中nextdataType的name值
	 */
	NEXT_DATATYPE(SynchConstants.HEADER_NEXT_DATATYPE),
	
	/**
	 * 返回客户端header中nextaction的name值
	 */
	NEXT_ACTION(SynchConstants.HEADER_NEXT_ACTION),
	
	/**
	 * 返回客户端header中nextaction的name值
	 */
	SERVER_TIMESTAMP(SynchConstants.HEADER_TIMESTAMP);
	
	private String value;
	
	HeaderName(String value){
		this.value = value;
	}
	
	@Override
	public String toString(){
		return this.value;
	}
}
